{
  "name": "Xcode Dark",
  "dark": true,
  "author": "Zihan Ma",
  "editorScheme": "/XcodeDark.xml",
  "ui": {
    "*": {
      "background": "#2a2a2a",
      "borderColor": "#1e1e1e"
    },
    "Borders": {
      "color": "#1e1e1e",
      "ContrastBorderColor": "#1e1e1e"
    },
    "VersionControl": {
      "Log": {
        "Commit": {
          "currentBranchBackground": "#292a2f",
          "hoveredBackground": "#383839"
        }
      }
    },
    "ToolWindow": {
      "Header": {
        "background": "#383839",
        "inactiveBackground": "#2a2a2a"
      },
      "Button": {
        "selectedBackground": "#383839",
        "hoverBackground": "#383839"
      }
    }
  }
}