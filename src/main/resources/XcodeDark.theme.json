{
  "name": "Xcode Dark",
  "dark": true,
  "author": "Zihan Ma",
  "editorScheme": "/XcodeDark.xml",
  "colors": {
    "primary": "#2a2a2a",
    "secondary": "#3b3a3a",
    "activeSelection": "#1767bc",
    "inactiveSelection": "#515b70",
    "border": "#1d1d1d"
  },
  "ui": {
    "*": {
      "arc": "8",
      "background": "primary",
      "selectionBackground": "activeSelection",
      "selectionInactiveBackground": "inactiveSelection",
      "underlineColor": "activeSelection",
      "borderColor": "border"
    },
    "ActionButton": {
      "hoverBackground": "border",
      "pressedBackground": "border"
    },
    "Borders": {
      "color": "border",
      "ContrastBorderColor": "border"
    },
    "Button": {
      "foreground": "#ffffff",
      "default": {
        "foreground": "#ffffff"
      }
    },
    "Editor": {
      "shortcutForeground": "#226fbf",
      "background": "secondary"
    },
    "EditorTabs": {
      "background": "secondary",
      "underlineHeight": 2
    },
    "SearchEverywhere": {
      "Tab": {
        "selectedBackground": "inactiveSelection"
      }
    },
    "SidePanel": {
      "background": "secondary"
    },
    "StatusBar": {
      "hoverBackground": "secondary"
    },
    "ToolWindow": {
      "Button": {
        "selectedBackground": "#131313"
      },
      "Header": {
        "background": "secondary",
        "inactiveBackground": "secondary"
      }
    },
    "VersionControl": {
      "Log": {
        "Commit": {
          "currentBranchBackground": "secondary",
          "hoveredBackground": "inactiveSelection"
        }
      }
    },
    "WelcomeScreen": {
      "SidePanel": {
        "background": "secondary"
      },
      "Projects": {
        "actions": {
          "background": "inactiveSelection"
        }
      }
    }
  },
  "icons": {
    "ColorPalette": {
      "Actions.Grey": "#a4a1b3",
      "Actions.Red": "#c63a5d",
      "Actions.Yellow": "#caba2d",
      "Actions.Green": "#25ad6b",
      "Actions.Blue": "#4d85ff",
      "Actions.GreyInline.Dark": "#9f99bfb3",
      "Objects.Grey": "#9790ad",
      "Objects.RedStatus": "#dd3962",
      "Objects.Red": "#c63a5d",
      "Objects.Pink": "#f98b9e",
      "Objects.Yellow": "#caba2d",
      "Objects.Green": "#239e62",
      "Objects.Blue": "#598bff",
      "Objects.Purple": "#af71e0",
      "Objects.BlackText": "#000000ff",
      "Objects.YellowDark": "#988c26",
      "Objects.GreenAndroid": "#78c257"
    }
  }
}